      "source": [
        "a=1042000\n",
        "b=702648265\n",
        "print(\"armstrong numbers between\",a,\"and\",b,\"are:\")\n",
        "for num in range(a,b+1):\n",
        "  order=len(str(num))\n",
        "  sum=0\n",
        "  temp=num\n",
        "  while temp>0:\n",
        "    digit=temp%10\n",
        "    sum+=digit**order\n",
        "    temp//10\n",
        "  if num==sum:\n",
        "    print(num)"
      ],
